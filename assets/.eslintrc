// .eslintrc

{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest/globals": true,
  },
  "plugins": [
    "react",
    "jest"
  ],
  "rules": {
    "jsx-a11y/anchor-is-valid": [ "error", {
      "component": ["Link"],
      "aspects": ["noHref"]
    }],
    "react/forbid-prop-types": "off",
    "no-console": 0,
  }
}
